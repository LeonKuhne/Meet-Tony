{
  "manifest_version": 3,
  "name": "Meet Tony",
  "version": "1.1",
  "description": "Fly through websites with frequent action suggestions.",
  "icons": {
    "48": "icons/tony-48.png"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "lib/jquery.min.js",
        "meet_tony.js"
      ],
      "run_at": "document_end"
    }
  ]
}
