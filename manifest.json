{

  "manifest_version": 1,
  "name": "Zoom",
  "version": "1.0",

  "description": "Quickly fly through websites with predictive navigation.",

  "icons": {
    "48": "icons/zoom-48.png"
  },

  "content_scripts": [
    {
      "matches": ["*://*.mozilla.org/*"],
      "js": ["zoom.js"]
    }
  ]

}
